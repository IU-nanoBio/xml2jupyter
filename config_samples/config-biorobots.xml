<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-1000.0</x_min>
		<x_max>1000.0</x_max>
		<y_min>-1000.0</y_min>
		<y_max>1000.0</y_max>
		<z_min>-10.0</z_min>
		<z_max>10.0</z_max>
		<dx>20.0</dx>
		<dy>20.0</dy>
		<dz>20.0</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">2880.0</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder>  

		<full_data>
			<interval units="min">2</interval>
			<enable>True</enable>
		</full_data>
		
		<SVG>
			<interval units="min">2</interval>
			<enable>True</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<user_parameters>
		<random_seed type="int" units="dimensionless">0</random_seed> 

 		<GUI_block border="true"> 
		<cargo_signal_D type="double" units="micron/min^2">1600.0</cargo_signal_D>
		<cargo_signal_decay type="double" units="1/min">0.8</cargo_signal_decay> 
		<director_signal_D type="double" units="micron/min^2">1300.0</director_signal_D>
		<director_signal_decay type="double" units="1/min">0.19</director_signal_decay> 
 		</GUI_block> 
		
		
		<elastic_coefficient type="double" units="1/min">0.12</elastic_coefficient>

		<worker_motility_persistence_time type="double" units="min">5.4</worker_motility_persistence_time>
		<worker_migration_speed type="double" units="micron/min">5.4</worker_migration_speed>
		<attached_worker_migration_bias type="double" units="min">1.5</attached_worker_migration_bias>
		<unattached_worker_migration_bias type="double" units="min">1.0</unattached_worker_migration_bias>

		 
		<number_of_directors type="int" units="none">20</number_of_directors> 
		<number_of_cargo_clusters type="int" units="none">110</number_of_cargo_clusters>
		<number_of_workers type="int" units="none">50</number_of_workers>
		
		 
		<drop_threshold type="double" units="dimensionless">0.4</drop_threshold> 
		
		 
		<worker_color type="string" units="none">red</worker_color>
		<cargo_color type="string" units="none">blue</cargo_color>
		<director_color type="string" units="none">limegreen</director_color> 
	
	</user_parameters>
	
</PhysiCell_settings>
